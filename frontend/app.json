{
  "expo": {
    "name": "LAF - Dieta & Treino",
    "slug": "laf-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "laf",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "leoluccand",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laf.git",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      },
      "entitlements": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.laf.app",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#10B981"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "BACKEND_URL": "https://laf-app.onrender.com"
    }
  }
}
